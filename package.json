{
  "name": "blog",
  "version": "1.0.0",
  "description": "My blog",
  "author": "mnao305",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "commit": "git-cz",
    "mdlint": "markdownlint **/*.md -c .markdownlint.json --ignore node_modules",
    "ogp": "npx ts-node scripts/linkCardGenerator.ts"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint",
    "*.md": "npm run mdlint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.10.0",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/composition-api": "^0.13.5",
    "@nuxtjs/feed": "^2.0.0",
    "@nuxtjs/gtm": "^2.3.2",
    "@nuxtjs/pwa": "^3.2.2",
    "jsdom": "^16.4.0",
    "nuxt": "^2.14.7",
    "rss-parser": "^3.9.0",
    "vue-social-share-component": "^1.1.4",
    "vue-tweet-embed": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.7",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^4.0.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "@nuxtjs/vercel-builder": "^0.17.12",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/xml2js": "^0.4.5",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.2.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "markdownlint-cli": "^0.23.2",
    "material-design-icons-iconfont": "^5.0.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "sass-loader": "^8.0.2",
    "sharp": "^0.25.4",
    "stylelint": "^13.7.2"
  }
}
